{
  "name": "ts-template",
  "description": "Typescript project skeleton",
  "version": "0.0.1",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "typings": "dist/index.d.ts",
  "repository": "https://github.com/Rawnly/ts-template",
  "license": "MIT",
  "author": "Federico Vitale <main@fedevitale.dev> (https://fedevitale.dev)",
  "private": false,
  "scripts": {
    "tscheck": "ttsc --noEmit",
    "dev": "node build.js",
    "build": "NODE_ENV=production node build.js",
    "dev:watch": "node build.js --watch",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "prebuild": "rm -rf dist && npm run tscheck",
    "pretest": "eslint --fix src/**/*"
  },
  "dependencies": {
    "type-fest": "^1.2.1"
  },
  "devDependencies": {
    "@types/jest": "^26.0.23",
    "@types/node": "^15.12.4",
    "@typescript-eslint/eslint-plugin": "^4.28.1",
    "@typescript-eslint/parser": "^4.28.1",
    "chalk": "^4.1.2",
    "cowsay": "^1.5.0",
    "esbuild": "^0.13.3",
    "esbuild-node-externals": "^1.3.0",
    "eslint": "^7.29.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^24.3.6",
    "jest": "^27.0.5",
    "npm-dts": "^1.3.10",
    "prettier": "^2.2.0",
    "ts-jest": "^27.0.3",
    "ttypescript": "^1.5.12",
    "typescript": "^4.3.4",
    "typescript-transform-paths": "^3.0.0"
  }
}
